[
    {
        "name": "sheepit-backend",
        "image": "sheepit/sheepit_backend:latest",
        "cpu": 0,
        "memory": 256,
        "portMappings": [
            {
                "containerPort": 80,
                "hostPort": 8080,
                "protocol": "tcp"
            }
        ],
        "environment": [
            {
                "name": "ASPNETCORE_ENVIRONMENT",
                "value": "production"
            }
        ],
        "essential": true
    },
    {
        "name": "mongodb-backend",
        "image": "mongo:4.1",
        "cpu": 0,
        "memory": 256,
        "portMappings": [
            {
                "containerPort": 27017,
                "hostPort": 27017,
                "protocol": "tcp"
            }
        ],
        "essential": true
    }
]